    "url": "https://github.com/luanhaipeng/rebix-utils/issues"
  },
  "homepage": "https://github.com/luanhaipeng/rebix-utils#readme",
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-core": "^5.8.38",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^5.4.2",
    "case-sensitive-paths-webpack-plugin": "^1.1.4",
    "classname": "^0.0.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "es3ify": "^0.2.0",
    "eslint": "^1.7.1",
    "eslint-config-rackt": "1.1.0",
    "eslint-plugin-react": "^3.6.3",
    "glob": "^6.0.4",
    "html-webpack-plugin": "^2.28.0",
    "isparta": "4.0.0",
    "istanbul": "^0.3.17",
    "jsdom": "~5.4.3",
    "react": "^15.4.0",
    "react-dom": "^15.4.2",
    "rimraf": "^2.3.4",
    "style-loader": "=0.13.1",
    "webpack": "=2.2.1"
